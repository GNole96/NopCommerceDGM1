package Automation;

public class GnEmpliment {
	
	
	private char[] a;

	void MB() {
		//implicite castig
		
	 byte a = 20;
	 int z = a;   // data Widding 
	 
	 //explicite Casting 
	 
	 long x = 999999999;
	 
	  x = (int)a;
	 
	  System.out.println(a);
	 
	}

	public static void main(String[] args) {
		GnEmpliment g = new GnEmpliment();
		
		g.MB();
	}
}
