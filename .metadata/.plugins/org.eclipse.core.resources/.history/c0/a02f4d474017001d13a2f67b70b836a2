package TestPackage1;

public class forloop {
	
	public static void main(String[]args) {
		
		int a,b,c,d,e,f,g,h;
		
		for(a=7;a>=1;a--) {
			for(b=1;b<=a;b++) {
				System.out.print("0");		
			}
			for(c=14;c>=2*a;c--) {
				System.out.print(1);	
			}
			for(b=2;b<=2*a;b++) {
				System.out.print("0");
				}
			for(c=14;c>=2*a;c--) {
				System.out.print(1);
				}
			System.out.println();
		}
/////////////////////////////////////////////////////////////////////////////////////////////		
		
		
		
		
		
		
		
		
		
		
		
	}
}